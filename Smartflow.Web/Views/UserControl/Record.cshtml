@model System.Collections.Generic.IEnumerable<Smartflow.Web.Code.Record>
@if (Model.Count() > 0)
{
    <table class="table flow-table">
        <tr>
            <td colspan="2" style="text-align:right;border-top:solid 1px #eee;border-right:solid 1px #eee;">
                <a href="#" onclick="openWinWorkflowImage('@Url.Action("WorkflowImage", new { instanceID = ViewBag.InstanceID })')">查看流程图</a>
            </td>
        </tr>
        @foreach (Smartflow.Web.Code.Record rec in Model)
        {
            <tr>
                <td class="flow-node">@rec.NODENAME</td>
                <td class="flow-message">@rec.MESSAGE</td>
            </tr>
        }
    </table>
}