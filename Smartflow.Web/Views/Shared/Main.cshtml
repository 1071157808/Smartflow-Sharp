@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>工作流使用示例</title>
    <link href="~/Content/smartflow.css" rel="stylesheet" />
    <style type="text/css">
            body{
              font-family:"Lucida Grande", Lucida Sans Unicode,Hiragino Sans GB, WenQuanYi Micro Hei, Verdana, Aril, sans-serif;
              font-size:14px;
              color:#000;
              }
        .top{
            height:80px;
            background-color:#0094ff;
        }
        .content{
            position:absolute;
            top:80px;
            left:0;
            right:0;
            bottom:0;
        }
        .left{
            width:200px;
            float:left;
            height:100%;
            background-color:#0894e5;
        }
        .left:after
        {
            content: ".";
            display: block;
            visibility: hidden;
            height: 1%;
            clear: both;
        }
        .right{
            height:100%;
            margin-left:200px;
        }
        iframe{
            width:100%;
            height:100%;
        }

        ul{
            list-style:none;
            color:#fff;
        }
        ul li{
            height:40px;
            line-height:40px;
            text-align:center;
            
        }
        ul li:hover{
            background-color:#d4e2ed;
        }
        ul li a{
           color:#fff;
           font-size:20px;
           text-decoration:none;
        }
    </style>
    <script type="text/javascript">

        function go(url) {
            document.getElementById("iframeContent").src = url;
        }

    </script>
</head>
<body>
    <div class="top"> 
        
    </div>

    <div class="content">

        <div class="left">
            <ul>
                <li>
                    <a href="#" onclick="go('@Url.Action("List","Default")')">流程设计器</a>
                </li>
                <li>
                    <a href="#" onclick="go('@Url.Action("ApplyList", "Form")')">表单申请</a>
                </li>
            </ul>
        </div>
        <div class="right">
            <iframe id="iframeContent" frameborder="0" src="@Url.Action("List","Default")"></iframe>
        </div>
    </div>

</body>
</html>
