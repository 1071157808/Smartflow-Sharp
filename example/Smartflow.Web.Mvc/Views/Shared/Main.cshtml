@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Smartflow-Tech 工作流平台</title>
    <link href="~/Content/main.css" rel="stylesheet" />
    <script type="text/javascript">
        function go(url) {
            document.getElementById("iframeContent").src = url;
        }
    </script>
</head>
<body class="mainFrame">
    <div class="top">
        <span>Smartflow-Sharp 工作流平台</span>
    </div>
    <div class="content">
        <div class="left">
            <ul class="menu">
                <li>
                    <a href="javascript:go('@Url.Action("ApplyList", "Apply")')">表单申请</a>
     
                </li>
                <li>
                    <a href="javascript:go('@Url.Action("List", "Default")')">流程设计器</a>
                </li>
            </ul>
        </div>
        <div class="right">
            <iframe id="iframeContent" style="width:100%;
                    height:100%;" frameborder="0" src="@Url.Action("ApplyList", "Apply")"></iframe>
        </div>
    </div>
</body>
</html>
